{
  "name": "eth-netstats",
  "description": "Hcd Network Intelligence dashboard",
  "version": "0.0.9",
  "private": true,
  "engines": {
    "node": "0.12.0",
    "npm": "2.5.1"
  },
  "apps": [
  {
    "name": "Hcd Dashboard",
    "script": "./bin/www",
    "node_args": [
      "--harmony"
    ],
    "args": [
      "decred_dashboard"
    ],
    "env": {
      "COMMON_ENV_VAR": "true",
      "NODE_ENV": "production"
    }
  }],
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "1.13.3",
    "chalk": "1.1.1",
    "d3": "3.5.6",
    "debug": "2.2.0",
    "express": "4.13.3",
    "geoip-lite": "1.1.6",
    "jade": "1.11.0",
    "lodash": "3.10.1",
    "pm2": "^1.1.1",
    "primus": "3.2.1",
    "primus-emit": "0.1.2",
    "primus-spark-latency": "0.1.1",
    "ws": "0.8.0"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.12.3",
    "grunt-contrib-jade": "^0.14.1",
    "grunt-contrib-uglify": "^0.9.1"
  }
}
